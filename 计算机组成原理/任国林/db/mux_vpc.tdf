--synthesis_resources = lut 2 
SUBDESIGN mux_vpc
( 
	data[3..0]	:	input;
	result[1..0]	:	output;
	sel[0..0]	:	input;
) 
VARIABLE 
	result_node[1..0]	: WIRE;
	sel_node[0..0]	: WIRE;
	w_data18w[1..0]	: WIRE;
	w_data4w[1..0]	: WIRE;

BEGIN 
	result[] = result_node[];
	result_node[] = ( ((sel_node[] & w_data18w[1..1]) # ((! sel_node[]) & w_data18w[0..0])), ((sel_node[] & w_data4w[1..1]) # ((! sel_node[]) & w_data4w[0..0])));
	sel_node[] = ( sel[0..0]);
	w_data18w[] = ( data[3..3], data[1..1]);
	w_data4w[] = ( data[2..2], data[0..0]);
END;
